{
    "version": 2,
    "builds": [
      {
        "src": "main.ts",
        "use": "@vercel/node" 
      },
      {
        "src": "pages/**/*.{js,ts,jsx,tsx}",
        "use": "@vercel/next"  
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.ts"
      }
    ],
    "devCommand": "npm run start:dev" 
  }